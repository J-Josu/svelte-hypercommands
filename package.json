{
  "name": "svelte-hypercommands",
  "version": "0.0.7",
  "description": "A headless Command Palette for hyper commands in your SvelteKit app",
  "keywords": [
    "sveltekit",
    "svelte",
    "command palette",
    "commands",
    "keybindings"
  ],
  "license": "MIT",
  "bugs": "https://github.com/J-Josu/svelte-hypercommands/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/J-Josu/svelte-hypercommands"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint ."
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./CommandPalette.svelte": {
      "types": "./dist/default/CommandPalette.svelte.d.ts",
      "svelte": "./dist/default/CommandPalette.svelte"
    }
  },
  "typesVersions": {
    ">4.0": {
      "index.d.ts": [
        "./dist/index.d.ts"
      ],
      "CommandPalette.svelte": [
        "./dist/default/CommandPalette.svelte.d.ts"
      ]
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "svelte": "^4.0.0",
    "@sveltejs/kit": "^1.27.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "publint": "^0.1.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11"
  },
  "type": "module"
}
